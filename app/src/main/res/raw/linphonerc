[net]
#Because dynamic bitrate adaption can increase bitrate, we must allow "no limit"
download_bw=0
upload_bw=0
mtu=1300

[rtp]
audio_rtp_port=7076
video_rtp_port=9078
audio_jitt_comp=60
video_jitt_comp=60
nortp_timeout=20
disable_upnp=1

[sip]
srtp_crypto_suites=AES_CM_256_HMAC_SHA1_80, AES_CM_256_HMAC_SHA1_32, AES_CM_128_HMAC_SHA1_32, AES_CM_128_HMAC_SHA1_80
